end

function map()

gg.clearResults()

gg.setRanges(32)

gg.searchNumber("25", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)

revert = gg.getResults(110000, nil, nil, nil, nil, nil, nil, nil, nil)

gg.editAll("-11", gg.TYPE_FLOAT)

end

function op()

gg.clearResults()

